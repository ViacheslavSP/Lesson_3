public class CompareNumbers {
    public static void compareNumbers(int a, int b) {
    if (a >= b) {
    System.out.println("a >= b");
    } else {
    System.out.println("a < b");
    }
    }
   
    public static void main(String args) {
    int a = 5; //
    int b = 3; //
    compareNumbers(a, b);
    }
   }
