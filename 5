public class CheckSumRange {
    public static boolean checkSumRange(int a, int b) {
    int sum = a + b;
    return sum >= 10 && sum <= 20;
    }
   
    public static void main(String args) {
    int a = 5; // Пример значения для a
    int b = 7; // Пример значения для b
    boolean result = checkSumRange(a, b);
   
    if (result) {
    System.out.println("Сумма в диапазоне от 10 до 20");
    } else {
    System.out.println("Сумма вне диапазона от 10 до 20");
    }
    }
   }
