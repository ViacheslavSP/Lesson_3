public class CheckSumSign {
    public static void main(String[] args) {
        int a = 5; // Пример значения для a
        int b = 3; // Пример значения для b
        int sum = a + b;

        if (sum >= 0) {
            System.out.println("Сумма положительная");
        } else {
            System.out.println("Сумма отрицательная");
        }
    }
}
